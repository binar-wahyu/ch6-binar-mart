<button type="button" onclick="history.back()" class="btn btn-light">
  <i class="bi-arrow-left"></i>
  Kembali
</button>

<h3 class="my-3"><%= pageTitle %></h3>

<div class="table-responsive mt-3">
  <table class="table table-bordered">
    <tbody>
      <tr>
        <th scope="row">No. Telp</th>
        <td><%= supplier.phoneNumber %></td>
      </tr>
      <tr>
        <th scope="row">Email</th>
        <td><%= supplier.email %></td>
      </tr>
      <tr>
        <th scope="row">Alamat</th>
        <td><%= supplier.address %></td>
      </tr>
      <tr>
        <th scope="row">Tanggal Bergabung</th>
        <td><%= supplier.joinDate %></td>
      </tr>
      <tr>
        <th scope="row">Data Dibuat</th>
        <td><%= supplier.createdAt %></td>
      </tr>
      <tr>
        <th scope="row">Data Diperbarui</th>
        <td><%= supplier.updatedAt %></td>
      </tr>
    </tbody>
  </table>
</div>